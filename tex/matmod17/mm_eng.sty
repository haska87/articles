% Жирный шрифт для бедных. (См. D. Knuth, The TeXbook).
% Вариант с правильным восьмиугольником.
\def\sqrtfromtwo{.7071067811865}
\def\fatsmb#1#2{\dimen0 #2\raise\dimen0\copy#1\kern-\wd#1%
\raise-\dimen0\copy#1\kern-\wd#1\kern-\dimen0\copy#1\kern-\wd#1%
\kern2\dimen0\copy#1\kern-\wd#1\kern-\dimen0%
\kern-\sqrtfromtwo\dimen0\raise\sqrtfromtwo\dimen0\copy#1\kern-\wd#1%
\raise-\sqrtfromtwo\dimen0\copy#1\kern-\wd#1\kern\sqrtfromtwo\dimen0%
\kern\sqrtfromtwo\dimen0\raise\sqrtfromtwo\dimen0\copy#1\kern-\wd#1%
\raise-\sqrtfromtwo\dimen0\copy#1\kern-\wd#1\kern-\sqrtfromtwo\dimen0%
\box#1}

\def\pmb#1#2{\setbox0\hbox{$#1#2$}\fatsmb{0}{0.025em}}
\def\bm#1{\mathchoice{\pmb\dst{#1}}{\pmb\tst{#1}}{\pmb\sst{#1}}{\pmb\ssst{#1}}}

